{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df3030"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"Gen2link_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Gen2link'"
		},
		"sqllink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sqllink'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "s3030.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db3030"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Solutions"
		},
		"Gen2link_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa3030.dfs.core.windows.net/"
		},
		"sqllink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "s3030.database.windows.net"
		},
		"sqllink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db3030"
		},
		"sqllink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Solutions"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df3030_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"Gen2link_accountKey": {
						"value": "[parameters('Gen2link_accountKey')]"
					},
					"sqllink_password": {
						"value": "[parameters('sqllink_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Gen2link_properties_typeProperties_url": {
						"value": "[parameters('Gen2link_properties_typeProperties_url')]"
					},
					"sqllink_properties_typeProperties_server": {
						"value": "[parameters('sqllink_properties_typeProperties_server')]"
					},
					"sqllink_properties_typeProperties_database": {
						"value": "[parameters('sqllink_properties_typeProperties_database')]"
					},
					"sqllink_properties_typeProperties_userName": {
						"value": "[parameters('sqllink_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}